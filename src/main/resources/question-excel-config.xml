<?xml version="1.0" encoding="UTF-8"?>
<workbook>
    <worksheet name="Sheet1">
        <section startRow="0" endRow="0"/>
        <loop startRow="1" endRow="1" items="objs" var="obj" varType="com.longder.exam.entity.dto.QuestionExcelObject">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">obj.content</mapping>
                <mapping row="1" col="1">obj.type</mapping>
                <mapping row="1" col="2">obj.difficulty</mapping>
                <mapping row="1" col="3">obj.score</mapping>
                <mapping row="1" col="4">obj.choiceA</mapping>
                <mapping row="1" col="5">obj.choiceB</mapping>
                <mapping row="1" col="6">obj.choiceC</mapping>
                <mapping row="1" col="7">obj.choiceD</mapping>
                <mapping row="1" col="8">obj.answer</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>
</workbook>